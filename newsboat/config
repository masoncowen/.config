browser "open -a firefox %u"
bind-key j down
bind-key k up
bind-key l open
bind-key RIGHT open
bind-key h quit
bind-key g home
bind-key G end
bind-key LEFT quit
bind-key z toggle-show-read-feeds
prepopulate-query-feeds yes
macro v set browser "mpv --really-quiet --no-terminal %u" ; open-in-browser ; set browser
macro d set browser "yt-dlp -P $HOME/Videos/temp -S res:1080 '%u'" ; open-in-browser ; set browser
