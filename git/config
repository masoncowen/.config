[mergetool "cvimdiff"]
  cmd = vim -S $VIM_CONFIG -f -d \
    -c '4wincmd w | wincmd J' \
    "$LOCAL" "$BASE" "$REMOTE" "$MERGED"
[merge]
	tool = cvimdiff
[alias]
	last = log -1 HEAD
	cf = config
	cfg = config --global
	st = status
	a = add
	d = diff
	cm = commit
	ps = push
	pl = pull
