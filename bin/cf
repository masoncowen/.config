#!/bin/sh
. $CONFIG/sh/.environment
case $1 in
  "v"|"vi"|"vim") $VISUAL $VIM_CONFIG
    ;;
  "t"|"tmux") $VISUAL $TMUX_CONFIG
    ;;
  "e"|"env"|"environment") $VISUAL $ENV
    ;;
  "a"|"alias") $VISUAL $ALIAS
    ;;
  "p"|"profile") $VISUAL $PROFILE
    ;;
  "f"|"func"|"function") $VISUAL $FUNC
	;;
esac
